<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" href="/assets/css/dashboard.css" type="text/css">
    <title>Dashboard</title>
</head>

<body>
    <h1 class="mt-5"> Hello </h1>
    
    <div class="app-parent mt-2" >
        <% users.forEach( i => { %>
        <div>
            <div>Username: <%=i.username%></div>
            <div>Password: <%=i.password%></div>
            <div>Firstname: <%=i.user_game_biodatum.first_name%></div>
            <div>Lastname: <%=i.user_game_biodatum.last_name%></div>
            <div>Birthplace: <%=i.user_game_biodatum.birthplace%></div>
            <div> <a href="/user/<%=i.id%>/edit">Update</a> | <a href="/user/<%=i.id%>/delete">Delete</a></div>
        </div>
        <% }) %>
    </div>


    <a class="app-btn-app" href="/adduser">Add User</a>
</body>

</html>